Tab:Button{
	Name = "Everything else",
	Description = nil,
	Callback = function() 
    
        local mt = getrawmetatable(game)
        setreadonly(mt, false)
        local index = mt.__index
        
        local Mods = {
            ["HeadshotDamage"] = 423,
            ["HeadshotCooldown"] = 0,
        }
        
        mt.__index = function(a,b)
            if Mods[tostring(a)] then
                if tostring(b) == "Value" then
                    return Mods[tostring(a)]
                end
            end
            return index(a,b)
        end
        
    end
}
